# Example of environment variables for the Frontier AI project

# GitHub Repository Configuration
GITHUB_REPO_URL=https://github.com/username/frontierAI
GITHUB_ACCESS_TOKEN=your_access_token_here

# Database Configuration
DATABASE_URL=sqlite:///data/evolution.db
DATABASE_TRACK_MODIFICATIONS=False

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=logs/system.log

# Evolution Engine Configuration
EVOLUTION_CYCLE_INTERVAL=60  # in seconds
MAX_EVOLUTION_CYCLES=100

# Metrics Configuration
METRICS_COLLECTION_INTERVAL=30  # in seconds

# UI Configuration
DASHBOARD_REFRESH_INTERVAL=5  # in seconds

# Other Configuration
ENABLE_AUTO_UPGRADES=True
NOTIFICATION_EMAIL=your_email@example.com