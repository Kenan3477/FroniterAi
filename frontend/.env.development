# Frontier Conversational Dashboard - Development Environment

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000

# App Configuration
REACT_APP_NAME=Frontier Dashboard
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_VOICE_INPUT=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_AUTO_REFRESH=true

# Debug Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Authentication
REACT_APP_TOKEN_STORAGE_KEY=frontier_token
REACT_APP_REMEMBER_ME_KEY=frontier_remember

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_DEFAULT_LANGUAGE=en

# Performance
REACT_APP_MAX_CONVERSATION_HISTORY=100
REACT_APP_TYPING_DELAY=150
REACT_APP_AUTO_SAVE_INTERVAL=30000

# External Services (Mock for development)
REACT_APP_SENTRY_DSN=
REACT_APP_ANALYTICS_ID=
REACT_APP_STRIPE_PUBLIC_KEY=
