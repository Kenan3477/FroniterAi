# Kennex Backend Environment Variables for Railway

# Node Environment
NODE_ENV=production

# Database (Railway will provide PostgreSQL)
DATABASE_URL=postgresql://user:password@host:5432/database

# Redis (Railway will provide Redis)
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# API
API_PORT=3002

# Flow Engine
FLOW_ENGINE_TIMEOUT=30000
FLOW_ENGINE_MAX_STEPS=100

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_API_KEY=your_twilio_api_key
TWILIO_API_SECRET=your_twilio_api_secret
TWILIO_SIP_DOMAIN=your-domain.sip.twilio.com
TWILIO_TWIML_APP_SID=your_twiml_app_sid
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# Backend URL (Railway will provide this)
BACKEND_URL=https://your-app.up.railway.app

# Optional
OPENAI_API_KEY=
